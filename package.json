{
  "name": "waffle-desktop",
  "version": "0.0.5",
  "description": "Wrapper for Waffle.io with desktop notifications",
  "author": "Jeff Andersen <@jeffandersen>",
  "license": "MIT",
  "readme": "https://github.com/erdembas/waffle-desktop",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/erdembas/waffle-desktop.git"
  },
  "scripts": {
    "start": "electron .",
    "clean": "rm -rf release/",
    "pack:win": "electron-packager . \"Waffle Desktop for Windows\" --out=dist/win --platform=win32 --arch=ia32"
  },
  "keywords": [
    "waffle",
    "github issues",
    "desktop app"
  ],
  "main": "src/main.js",
  "devDependencies": {
    "electron-builder": "5.14.1",
    "electron-packager": "7.1.0",
    "electron-prebuilt": "1.2.7"
  },
  "dependencies": {
    "electron": "0.4.1",
    "electron-default-menu": "0.2.0",
    "electron-json-storage": "2.0.0",
    "lodash": "4.13.1",
    "request": "2.72.0",
    "semver": "5.2.0"
  }
}